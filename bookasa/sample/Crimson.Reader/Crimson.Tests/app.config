                        <transport clientCredentialType="None" proxyCredentialType="None"
                            realm="" />
                        <message clientCredentialType="UserName" algorithmSuite="Default" />
                    </security>
                </binding>
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://www.flash-db.com/services/ws/flashCDDB.php"
                binding="basicHttpBinding" bindingConfiguration="FlashCDDBBinding"
                contract="CDDB.FlashCDDBPort" name="FlashCDDBPort" />
            <endpoint address="http://ws.strikeiron.com/TeleSign/televerification/TelesignAPI"
                binding="basicHttpBinding" bindingConfiguration="TelesignAPISoap"
                contract="PhoneValidation.TelesignAPISoap" name="TelesignAPISoap" />
        </client>
    </system.serviceModel>
</configuration>